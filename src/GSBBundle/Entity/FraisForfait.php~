<?php
namespace GSBBundle\Entity;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 */
class FraisForfait
{
    /**
     * @ORM\Id
     * @ORM\Column(type="string")
     *
     */
    private $id;

    /**
     * @ORM\Column(type="string", nullable=true)
     */
    private $libelle;

    /**
     * @ORM\Column(type="decimal", nullable=true)
     */
    private $montant;
}
